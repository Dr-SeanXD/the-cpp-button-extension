{
  "name": "the-c---button",
  "publisher": "Dr-SeanXD",
  "displayName": "The C++ Button",
  "description": "An extension to compile and run C++ files",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.94.0"
  },
  "main": "./extension.js",
  "activationEvents": [
  "onCommand:the-c---button.button"
  ],
  "contributes": {
    "commands": [
      {
        "command": "the-c---button.button",
        "title": "ðŸš€ Run C++ ðŸ’»"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "the-c---button.button",
          "when": "resourceExtname == .cpp",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}